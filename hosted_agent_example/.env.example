# Azure AI Foundry Configuration
# Copy this to .env and fill in your values

# Azure OpenAI Settings
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_CHAT_DEPLOYMENT_NAME=gpt-5.1-mini
AZURE_OPENAI_API_VERSION=2024-08-01-preview

# Azure AI Foundry Project
AZURE_AI_PROJECT_ENDPOINT=https://aq-ai-foundry-sweden-central.services.ai.azure.com/api/projects/firstProject

# Authentication
# DefaultAzureCredential will use these automatically
AZURE_CLIENT_ID=your-client-id
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_SECRET=your-client-secret

# OpenTelemetry (for tracing)
OTEL_EXPORTER_ENDPOINT=http://localhost:4318
APPLICATIONINSIGHTS_CONNECTION_STRING=your-app-insights-connection-string

# Agent Configuration
AGENT_NAME=my-hosted-agent
AGENT_VERSION=1.0.0
